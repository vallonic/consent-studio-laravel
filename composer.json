{
    "name": "consent-studio/laravel",
    "description": "Laravel package for Consent Studio CMP (Consent Management Platform) integration",
    "keywords": ["laravel", "consent", "gdpr", "cookie", "consent-management", "cmp"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Consent Studio",
            "email": "support@consent.studio"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ConsentStudio\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConsentStudio\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConsentStudio\\Laravel\\ConsentStudioServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
